rid-sizer-nested\"><\/span>\n\t\t\t\t\t\n\t\t\t\t\t                \n\t\t\t                        \t\t\t                        \n\t\t\t\t\t\t\t<div class=\"menu-item masonry-box-nested twelve\">\n\t\t\t\t\t\t\t        \n\t\t\t\t\t\t\t        <div class=\"inner-box \" >\n\n\t\t\t\t\t\t\t                <!-- thumbnail -->\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t  \t\t\t\t\t\t\t\t\t\t\t\t                \t\t\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t\t\t<!-- menu text -->\n\t\t\t\t\t\t\t                <div class=\"menu-text\">\n\n\t\t\t\t\t\t\t                \t<div class=\"menu-text-wrap\">\n\t\t\t\t\t\t\t\t                \t<div class=\"menu-text-left\">\n\t\t\t\t\t\t\t\t\t\t                <h6 class=\"menu-title use-body-font\" style=\"color:#3a3a3b\">French Press Coffee\t\t\t\t\t\t\t\t\t\t                <\/h6>\n\t\t\t\t\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t\t\t\t        <\/div>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t<div class=\"menu-price use-body-font\" style=\"color:#3a3a3b\" >\n\t\t\t\t\t\t\t\t\t\t\t    \t\t2.5\t\t\t\t\t\t\t\t\t\t\t\t<\/div>\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t        <\/div>\n\n\t\t\t\t\t\t\t\t\t<\/div> \n\n\t\t\t\t\t\t\t\t<\/div><!-- .inner-box -->\n\t\t\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t\t<div class=\"clear\"><\/div>\n\t\t\t\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t<\/div><!-- .menu-item -->\n\n\t\t\t\t\t                \n\t\t\t                        \t\t\t                        \n\t\t\t\t\t\t\t<div class=\"menu-item masonry-box-nested twelve\">\n\t\t\t\t\t\t\t        \n\t\t\t\t\t\t\t        <div class=\"inner-box \" >\n\n\t\t\t\t\t\t\t                <!-- thumbnail -->\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t  \t\t\t\t\t\t\t\t\t\t\t\t                \t\t\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t\t\t<!-- menu text -->\n\t\t\t\t\t\t\t                <div class=\"menu-text\">\n\n\t\t\t\t\t\t\t                \t<div class=\"menu-text-wrap\">\n\t\t\t\t\t\t\t\t                \t<div class=\"menu-text-left\">\n\t\t\t\t\t\t\t\t\t\t                <h6 class=\"menu-tit